{
  "environments": [
    {
      "name": "production",
      "configOverride": {},
      "schedules": [
        {
          "name": "daily project refresh",
          "cron": "0 16 * * mon,tue,wed,thu,fri,sat,sun",
          "options": {
            "fullRefresh": false
          },
          "disabled": false,
          "notify": [
            {
              "channel": "xa-dataform",
              "statuses": [
                "SUCCESS",
                "FAILURE"
              ]
            }
          ]
        }
      ],
      "gitRef": "master"
    }
  ],
  "notificationChannels": [
    {
      "name": "xa-dataform"
    }
  ]
}